<SyntaxDefinition name="LF2-Data" extensions=".dat">
  <Default forecol="Black" backcol="White" />
  <Regex name="CommentOut" forecol="DimGray" pattern="#.*" />
  <Regex name="File" forecol="#FF008E61" pattern="((?&lt;=\s+|^)[A-Za-z_\-\/\\0-9]+)\.(?=.*[a-zA-Z])[^\s]{2,4}" />
  <Regex name="BitmapFile" forecol="DarkRed" pattern="file\((?=\s*\d+\s*\-\s*\d+\s*\):)|(?&lt;=file\(\s*\d+\s*\-\s*\d+\s*)\):" />
  <Regex name="FileNumbers" forecol="Fuchsia" pattern="(?&lt;=file\()\s*\d+\s*\-\s*\d+\s*(?=\):)" />
  <Regex name="NumberDataSpecial" forecol="Fuchsia" pattern="(?&lt;=\d+\s*~\s*)\d+" example="100~199" />
  <Regex name="Tilde" forecol="DarkRed" bold="1" pattern="(?&lt;=\d+\s*)~(?=\s*\d+)" example="100~199" />
  <Regex name="TildeArgs" forecol="Gray" italic="1" pattern="(?&lt;=\d+\s*~\s*\d+\s+)(\w+\s*){1,2}" example="100~199 drop weapon" />
  <Regex name="FrameName" forecol="Green" bold="1" pattern="(?&lt;=\&lt;frame\&gt;\s*\d+\s+)[^\s\n\r]+" />

  <Regex name="SpecialFeature" forecol="#0094FF" bold="1" italic="1" pattern="(CEILING|FLOOR|TRUNCATE)\(.*?\)" />

  <Regex name="SpecialFeature" forecol="#0094FF" bold="1" pattern="(LIMIT\(\s*\d+\s*;\s*\d+\s*\)|(MIN|MAX)\(\s*\d+\s*\))([+\-/*^()\d]+)*" />
  <Regex name="SpecialFeature" forecol="#0094FF" bold="1" pattern="(([+\-/*^()\d]+)+(\$|\?)([+\-/*^()\d]+)*|([+\-/*^()\d]+)*(\$|\?)([+\-/*^()\d]+)+)" />
  <!--
  <Regex name="SpecialFeatureLimit" forecol="#0094FF" bold="1" pattern="(LIMIT\((?=.*?;.*?\))|(MIN|MAX)\((?=.*\)))" />
  <Regex name="SpecialFeatureLimitTset" forecol="#0094FF" bold="1" pattern="(?&lt;=LIMIT|MIN|MAX\().*?(?=\))" />
  <Regex name="SpecialFeatureLimitEND" forecol="#0094FF" bold="1" pattern="((?&lt;=LIMIT\(.*?;.*?)\)|(?&lt;=(MIN|MAX)\(.*\)))" />
-->
 
  <SingleWordRegex forecol="Fuchsia">
    <Pattern example="3.1415">[-]?(\d+(?:(\.|\,)\d+)?)</Pattern>
    <Pattern example=".1415">[-]?(\.|\,)\d+</Pattern>
  </SingleWordRegex>

	<Keywords forecol="Fuchsia">
		<Word>$</Word>
		<Word>?</Word>
	</Keywords>

